<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
           tools:ignore="MissingTranslation,ExtraTranslation">
{{#entries}}
{{#if isPlural}}
    <plurals name="{{key}}">
      {{#if values.zero}}<item quantity="zero">{{values.zero}}</item>
      {{/if}}{{#if values.one}}<item quantity="one">{{values.one}}</item>
      {{/if}}{{#if values.two}}<item quantity="two">{{values.two}}</item>
      {{/if}}{{#if values.few}}<item quantity="few">{{values.few}}</item>
      {{/if}}{{#if values.many}}<item quantity="many">{{values.many}}</item>
      {{/if}}{{#if values.other}}<item quantity="other">{{values.other}}</item>
      {{/if}}
    </plurals>
{{else}}
    <string name="{{key}}"{{#if doNotTranslate}} translatable="false"{{/if}}>{{value}}</string>
{{/if}}
{{/entries}}
</resources>